{
    "version": 2,
    "builds": [
        { "src": "index.js", "use": "@now/node-server" },
        { "src": "index.html", "use": "@now/static"}
        ],
    "routes": [
        {
        "src": "/api",
        "dest": "/index.js",
        "methods": ["GET"]
        },
        {
        "src": "/api/users",
        "dest": "/index.js",
        "methods": ["GET"]
        },
        {
        "src": "/api/user",
        "dest": "/index.js",
        "methods": ["POST"]
        }
    ]
}